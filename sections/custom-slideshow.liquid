<div
  data-section-id="{{ section.id }}"
  data-section-type="slideshow-section"
  {% if section.settings.parallax %}data-parallax="true"{% endif %}>

  {%- if section.blocks.size > 0 -%}
    <div class="slideshow-wrapper">
      {%- if section.settings.autoplay and section.settings.style == 'bars' and section.blocks.size > 1 -%}
        {%- style -%}
          [data-bars][data-autoplay="true"] .flickity-page-dots .dot:after {
            animation-duration: {{ section.settings.autoplay_speed | times: 1000 }}ms;
          }
        {%- endstyle -%}

        <button type="button" class="visually-hidden slideshow__pause" data-id="{{ section.id }}" aria-live="polite">
          <span class="slideshow__pause-stop">
            <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-pause" viewBox="0 0 10 13"><g fill="#000" fill-rule="evenodd"><path d="M0 0h3v13H0zM7 0h3v13H7z"/></g></svg>
            <span class="icon__fallback-text">{{ 'sections.slideshow.pause_slideshow' | t }}</span>
          </span>
          <span class="slideshow__pause-play">
            <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-play" viewBox="18.24 17.35 24.52 28.3"><path fill="#323232" d="M22.1 19.151v25.5l20.4-13.489-20.4-12.011z"/></svg>
            <span class="icon__fallback-text">{{ 'sections.slideshow.play_slideshow' | t }}</span>
          </span>
        </button>
      {%- endif -%}

      {%- assign natural_height = false -%}
      {%- assign natural_mobile_height = false -%}
      {%- if section.settings.section_height == 'natural' or section.settings.mobile_height == 'auto' -%}
        {% comment %}
          Get first image's aspect ratio
        {% endcomment %}
        {%- for block in section.blocks limit: 1 -%}
          {%- if block.settings.image != blank -%}
            {%- if section.settings.section_height == 'natural' %}
              {%- assign natural_height = true -%}
              {%- capture natural_height_ratio -%}{{ 100 | divided_by: block.settings.image.aspect_ratio }}%{% endcapture %}
            {%- endif -%}
          {%- endif -%}
          {%- if block.settings.image_mobile != blank -%}
            {%- if section.settings.mobile_height == 'auto' -%}
              {%- assign natural_mobile_height = true -%}
              {%- capture natural_mobile_height_ratio -%}{{ 100 | divided_by: block.settings.image_mobile.aspect_ratio }}%{% endcapture %}
            {%- endif -%}
          {%- endif -%}
        {%- endfor -%}
      {%- endif -%}


      {%- assign homepage_banner_customization = true -%}    
      {%- if homepage_banner_customization -%}
         {%- style -%}
              {%- if section.settings.custom_font == 'heading' -%}
                #shopify-section-{{ section.id }} .hero__title.cus_heading {
                  font-family:var(--typeHeaderPrimary), var(--typeHeaderFallback);
                }
              {%- elsif section.settings.custom_font == 'body' -%}
                #shopify-section-{{ section.id }} .hero__title.cus_heading {
                  font-family:var(--typeBasePrimary), var(--typeBaseFallback);
                }
              {%- elsif section.settings.custom_font == 'custom' -%}
                #shopify-section-{{ section.id }} .hero__title.cus_heading {
                  font-family: 'Friendly Schoolmates';
                }
              {%- elsif section.settings.custom_font == 'custom-new' -%}
                #shopify-section-{{ section.id }} .hero__title.cus_heading {
                  font-family: 'Times Sans Serif';
                }
              {%- endif -%}
              #shopify-section-{{ section.id }} .hero__link .btn.cus_btn {
                color: {{ section.settings.button_text_color }} !important;
                border-color: {{ section.settings.button_border_color }} !important;
                background-color: {{ section.settings.button_bg_color }} !important;
                text-transform: capitalize;
              }
              #shopify-section-{{ section.id }} .hero__link .btn.cus_btn:hover {
                color: {{ section.settings.button_text_color_hover }} !important;
                background-color: {{ section.settings.button_bg_color_hover }} !important;
                border-color: {{ section.settings.button_bg_color_hover }} !important;
              }
              .hero__subtitle .animation-contents {
                display: flex;
                flex-wrap: wrap;
                justify-content: flex-start;
              }

              .hero__subtitle .animation-cropper .animation-contents span {
                position: relative;
                padding-left: {{ section.settings.slider_text_icon_gap }}px;
                margin-top: 4px;
                margin-bottom: 4px;
                font-family: 'Urbanist';
              }
              .hero__subtitle .animation-cropper .animation-contents span::before {
                  content: '';
                  position: absolute;
                  display: block;
                  top: 0;
                  left: 0;
                  right: 0;
                  bottom: 0;
                  margin: 0;
                  padding: 0;
                  z-index: 9;
                  height: 28px;
                  width: 28px;
                  background-image: url(http://cdn.shopify.com/s/files/1/0587/6396/8710/{{ section.settings.slider_text_icon }});
                  background-repeat: no-repeat;
                  background-position: left center;
                  background-size: contain;
              }

              .index-section--hero.custom_slideshow {
                margin-top: -{{ section.settings.slider_overlap}}px;
              }
              @media screen and (max-width: 767px) {
                .hero__subtitle .animation-cropper .animation-contents span {
                  margin-right: {{ section.settings.slider_text_gap }}px !important;
                }
                .hero__subtitle .animation-cropper .animation-contents span {
                  padding-left: {{ section.settings.slider_text_icon_gap_mobile }}px;
                }
                .hero__subtitle .animation-contents {
                  justify-content: center;
                }
                .hero__subtitle .animation-cropper .animation-contents span::before {
                  height: 22px;
                  width: 22px;
                }
                #shopify-section-{{ section.id }} .hero__title.cus_heading {
                  font-size: {{ section.settings.slider_font_size_mobile }}px !important;
                  line-height: 46px;
                  letter-spacing: 2px;
                }
                #shopify-section-{{ section.id }} .hero__subtitle span {
                  font-size: {{ section.settings.slider_text_font_size_mobile }}px !important;
                  font-weight: 500;
                  line-height: 19.2px;
                  letter-spacing: 1px;
                  color: #273034CC;
                  min-height: 22px;
                  display: flex;
                  align-items: center;
                }
                #shopify-section-{{ section.id }}  .hero__subtitle .animation-contents {
                    gap: 10px;
                }
              }

              .custom_slideshow .hero__title, .custom_slideshow .hero__subtitle {
                color: {{ section.settings.slider_content_color}};
              }
              @media only screen and (min-width: 768px) {
                .slideshow__slide--c4d45fb1-221d-47c7-8db2-16a1e9f434e2 .hero__title {
                  font-size: {{ section.settings.slider_font_size_laptop }}px !important;
                }
                .custom_slideshow .hero__subtitle span {
                  font-size: {{ section.settings.slider_text_font_size_laptop }}px !important;
                }
                .hero__text-wrap:before {
                    width: 45%;
                }
                .hero__text-wrap.vertical-center.horizontal-center:before, .hero__text-wrap.vertical-bottom.horizontal-center:before {
                    width: 100% !important;
                }
                .hero__text-wrap.vertical-bottom.horizontal-right:before {
                  right: 0;
                }
                /* .hero__text-wrap.vertical-center.horizontal-right:before {
                    right: 0;
                    width: 100%;
                } */
              }
              @media only screen and (width: 768px) {
                .hero__subtitle .animation-cropper .animation-contents span::before {
                      top: 0 !important;
                }
                .hero__text-wrap:before {
                    width: 75%;
                }
              }
              @media only screen and (min-width: 769px) and (max-width: 1100px) {
                .hero__text-wrap:before {
                    width: 55%;
                    background-repeat: no-repeat;
                }
              }
              @media only screen and (min-width: 1600px) {
                .slideshow__slide--c4d45fb1-221d-47c7-8db2-16a1e9f434e2 .hero__title {
                  font-size: {{ section.settings.slider_font_size_desktop }}px !important;
                }
                .custom_slideshow .hero__subtitle span {
                  font-size: {{ section.settings.slider_text_font_size_desktop }}px !important;
                  margin-top: 10px !important;
                }
                #shopify-section-{{ section.id }} .hero__link .btn.cus_btn {
                  font-size : 16px !important;
                  line-height: 19.2px !important;
                  letter-spacing: 2px;
                }
              }
        {%- endstyle -%}
      {%- endif -%}

      {%- if natural_height -%}
        {%- style -%}
          @media only screen and (min-width: 768px) {
            .hero-natural--{{ section.id }} {
              height: 0;
              padding-bottom: {{ natural_height_ratio }};
            }
          }
        {%- endstyle -%}
      {%- endif -%}
      {%- if natural_mobile_height -%}
        {%- style -%}
          @media screen and (max-width: 767px) {
            .hero-natural-mobile--{{ section.id }} {
              height: 0;
              padding-bottom: {{ natural_mobile_height_ratio }};
            }
          }
        {%- endstyle -%}
      {%- endif -%}

      <div class="{% if natural_height %}cus_hero-natural hero-natural--{{ section.id }}{% endif %}{% if natural_mobile_height %} hero-natural-mobile--{{ section.id }}{% endif %}">
        <div id="Slideshow-{{ section.id }}"
          class="hero hero--{{ section.settings.section_height }} hero--{{ section.id }} hero--mobile--{{ section.settings.mobile_height }} loading loading--delayed"
          {% if natural_height %}
            data-natural="true"
          {% endif %}
          data-mobile-natural="{{ natural_mobile_height }}"
          data-autoplay="{{ section.settings.autoplay }}"
          data-speed="{{ section.settings.autoplay_speed | times: 1000 }}"
          {% if section.settings.style == 'arrows' %}
            data-arrows="true"
          {% endif %}
          {% if section.settings.style == 'dots' %}
            data-dots="true"
          {% endif %}
          {% if section.settings.style == 'bars' %}
            data-dots="true"
            data-bars="true"
          {% endif %}
          data-slide-count="{{ section.blocks.size }}">
          {%- for block in section.blocks -%}
            <div
              {{ block.shopify_attributes }}
              class="slideshow__slide slideshow__slide--{{ block.id }}"
              data-index="{{ forloop.index0 }}"
              data-id="{{ block.id }}">

              {%- style -%}
                .slideshow__slide--{{ block.id }} .hero__title {
                  font-size: {{ block.settings.title_size | times: 0.73 }}px;
                }
                @media only screen and (min-width: 768px) {
                  .slideshow__slide--{{ block.id }} .hero__title {
                    font-size: {{ block.settings.title_size | times: 0.85 }}px;
                  }
                }
                @media only screen and (min-width: 1600px) {
                  .slideshow__slide--{{ block.id }} .hero__title {
                    font-size: {{ block.settings.title_size }}px;
                  }
                }
              {%- endstyle -%}

              {%- liquid
                assign hero_text = false
                assign link_slide = false
                if block.settings.top_subheading != blank or block.settings.title != blank or block.settings.subheading != blank or block.settings.link_text != blank
                  assign hero_text = true
                endif
                if block.settings.link != blank and block.settings.link_2 == blank
                  assign link_slide = true
                endif

                assign has_mobile_image = false
                if block.settings.image_mobile != blank
                  assign has_mobile_image = true
                endif
              -%}

              <div class="hero__image-wrapper{% unless hero_text %} hero__image-wrapper--no-overlay{% endunless %}">

                {%- if section.settings.parallax -%}
                  <div class="parallax-container">
                    <div class="parallax-image">
                {%- endif -%}

                  {%- if block.settings.image != blank -%}
                    {%- assign img_url = block.settings.image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}

                    <img rel="preload" class="hero__image hero__image--{{ block.id }} {% if has_mobile_image %} small--hide{% endif %}"
                      src="{{ block.settings.image | img_url: 'master' }}"
                      loading="eager" fetchpriority="high"
                      data-src="{{ img_url }}"
                      data-aspectratio="{{ block.settings.image.aspect_ratio }}"
                      data-sizes="auto"
                      alt="{{ block.settings.image.alt | escape }}"
                      style="object-position: {{ block.settings.focal_point }}">
                    {%- if has_mobile_image -%}
                      {%- assign img_url_mobile = block.settings.image_mobile | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
                      <img rel="preload" class="hero__image hero__image--{{ block.id }} medium-up--hide"
                        src="{{ block.settings.image_mobile | img_url: 'master' }}"
                        loading="eager" fetchpriority="high"
                        data-src="{{ img_url_mobile }}"
                        data-aspectratio="{{ block.settings.image_mobile.aspect_ratio }}"
                        data-sizes="auto"
                        alt="{{ block.settings.image_mobile.alt | escape }}"
                        style="object-position: {{ block.settings.focal_point }}">
                    {%- endif -%}
                    <noscript>
                      <img loading="eager" fetchpriority="high" class="hero__image hero__image--{{ block.id }}"
                        src="{{ block.settings.image | img_url: '1400x' }}"
                        alt="{{ block.settings.image.alt | escape }}">
                    </noscript>
                  {%- else -%}
                    {%- if template == 'password' -%}
                      {%- assign password_image = 'password-page-background.jpg' | asset_url -%}
                      <img loading="lazy" class="hero__image lazyload" src="{{ password_image }}">
                    {%- else -%}
                      {{ 'lifestyle-1' | placeholder_svg_tag: 'placeholder-svg' }}
                    {%- endif -%}
                  {%- endif -%}

                {%- if section.settings.parallax -%}
                    </div>
                  </div>
                {%- endif -%}
              </div>

              {%- if link_slide -%}
                <a href="{{ block.settings.link }}" class="hero__slide-link" aria-hidden="true"></a>
              {%- endif -%}

              {%- if hero_text -%}
                <div class="hero__text-wrap {{ block.settings.text_align }}">
                  <div class="page-width">
                    <div class="hero__text-content {{ block.settings.text_align }}">
                      <div class="hero__text-shadow hero-content_inner">
                        {%- unless block.settings.top_subheading == blank -%}
                          <div class="hero__top-subtitle">
                            <div class="animation-cropper"><div class="animation-contents">
                              {{ block.settings.top_subheading | escape }}
                            </div></div>
                          </div>
                        {%- endunless -%}
                        {%- unless block.settings.title == blank -%}
                          <h2 class="h1 hero__title cus_heading">
                            <div class="animation-cropper"><div class="animation-contents">
                            {{ block.settings.title | newline_to_br }}
                            </div></div>
                          </h2>
                        {%- endunless -%}
                        {%- if block.settings.subheading or block.settings.link or block.settings.link_2 -%}
                          {%- unless block.settings.subheading == blank -%}
                            <div class="hero__subtitle">
                              <div class="animation-cropper"><div class="animation-contents">
                                {% if block.settings.subheading contains "|" %}
                                  {% assign subheading_parts = block.settings.subheading | split: '|' %}
                                  {% assign subheading_with_span = '<span>' | append: subheading_parts[0] | append: '</span>' %}
                                  {% for part in subheading_parts offset:1 %}
                                    {% assign subheading_with_span = subheading_with_span | append: '<span>' | append: part | append: '</span>' %}
                                  {% endfor %}
                                  {{ subheading_with_span }}
                                {% else %}
                                  {{ block.settings.subheading | escape }}
                                {% endif %}

                              </div></div>
                            </div>
                          {%- endunless -%}
                          {%- if block.settings.link_text != blank or block.settings.link_text_2 != blank -%}
                            <div class="hero__link">
                              {%- if block.settings.link_text != blank -%}
                                <a href="{{ block.settings.link }}" class="btn{% if block.settings.color_accent contains 'rgba(0,0,0,0)' %} btn--inverse{% endif %} cus_btn">
                                  {{ block.settings.link_text }}
                                </a>
                              {%- endif -%}
                              {%- if block.settings.link_text_2 != blank -%}
                                <a href="{{ block.settings.link_2 }}" class="btn{% if block.settings.color_accent contains 'rgba(0,0,0,0)' %} btn--inverse{% endif %} cus_btn cus_btn_reverse">
                                  {{ block.settings.link_text_2 }}
                                </a>
                              {%- endif -%}
                            </div>
                          {%- endif -%}
                        {%- endif -%}
                      </div>
                    </div>
                  </div>
                </div>
              {%- endif -%}
            </div>
          {%- endfor -%}
        </div>
      </div>
    </div>
  {%- endif -%}

  {%- if section.blocks.size == 0 -%}
    <div class="placeholder-noblocks">
      {{ 'home_page.onboarding.no_content' | t }}
    </div>
  {%- endif -%}
</div>
{% comment %}
<script>
  // Header Banner Titles Center | 2022-10-29
var b = (document.querySelector(".cus_title p").classList.contains("cus_p"));
if (b == true) {
    document.querySelector(".simple_cta-wrap a").style.position = 'relative';
} else {
    document.querySelector(".simple_cta-wrap a").style.position = 'absolute';
    document.querySelector(".simple_cta-wrap h4").style.margin = '0';
};


// var c = (document.querySelector(".simple_cta-section .page-width > div").classList.contains("simple_cta-section"));
// if (!c) {
//     document.querySelector(".custom_slideshow").style.margin = '-50px 0 0 0';
// }

var subLeft = (document.querySelector(".hero__text-content").classList.contains("horizontal-left"));
var subCenter = (document.querySelector(".hero__text-content").classList.contains("horizontal-center"));
var subRight = (document.querySelector(".hero__text-content").classList.contains("horizontal-right"));
if (subLeft == true ) {
  document.querySelector(".hero__subtitle .animation-cropper").style.justifyContent = 'flex-start';
  document.querySelector(".hero__subtitle .animation-contents").style.justifyContent = 'flex-start';
} 
